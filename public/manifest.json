{
  "name": "Discovemail",

  "description": "open your tabs",

  "version": "1.0.1",

  "manifest_version": 3,

  "host_permissions": ["<all_urls>"],

  "action": {
    "default_title": "Discovemail",

    "default_popup": "index.html",

    "default_icon": {
      "16": "Linkedin.png",

      "128": "Linkedin.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
